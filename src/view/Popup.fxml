<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="5.0" style="-fx-background-color: #366DA8;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainWindowController">
   <children>
      <HBox spacing="20.0">
         <children>
            <Label prefHeight="20.0" prefWidth="31.0" text="IP:" textFill="WHITE">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
               <font>
                  <Font name="Century Gothic" size="16.0" />
               </font>
            </Label>
            <TextField fx:id="ipInput" />
         </children>
         <VBox.margin>
            <Insets left="2.0" />
         </VBox.margin>
      </HBox>
      <HBox spacing="20.0">
         <children>
            <Label prefHeight="20.0" prefWidth="31.0" text="Port" textFill="WHITE">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
               <font>
                  <Font name="Century Gothic" size="16.0" />
               </font>
            </Label>
            <TextField fx:id="portInput" />
         </children>
         <VBox.margin>
            <Insets left="2.0" />
         </VBox.margin></HBox>
      <Button mnemonicParsing="false" onAction="#popupSubmit" prefHeight="37.0" prefWidth="206.0" style="-fx-background-color: #85C0FF;" text="Submit" textFill="#366da8">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
